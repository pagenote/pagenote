{
  "compilerOptions": {
    "target": "es5",
    "module": "ES2015",
    "strict": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
