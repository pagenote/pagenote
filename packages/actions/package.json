{
  "name": "@pagenote/actions",
  "version": "0.1.1-next.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "rowthan",
  "module": "dist/actions.esm.js",
  "size-limit": [
    {
      "path": "dist/actions.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/actions.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "@types/jest": "^29.5.11",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "size-limit": "^11.0.2",
    "ts-jest": "^29.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  }
}
